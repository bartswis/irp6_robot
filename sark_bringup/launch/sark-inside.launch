<launch>

  <group>
    <rosparam>
      ECManager:
        debug: false
        fault_autoreset: true
        sequent_synchro: true
        services_names: [irp6otm_1, irp6otm_2]
        regulators_names: [SarkofagRegulator1, SarkofagRegulator2]
        scheme_component_name: SarkScheme
      SarkDiag:
        hardware_label: Sarkofag
      SarkScheme:
        scheme_name: sarkofag_manager
      SarkofagSplineTrajectoryGeneratorJoint:
        number_of_joints: 2
      SarkofagSplineTrajectoryActionJoint:
        joint_names: [joint1, joint2]
        lower_limits: [-1000.0, -1000.0]
        upper_limits: [1000.0, 1000.0]
      SarkofagMotorParameters:
        lower_limits: [-100000.0, -100000.0]
        upper_limits: [100000.0, 100000.0]
        enc_res: [20000.0, 20000.0]
      SarkofagSplineTrajectoryGeneratorMotor:
        number_of_joints: 2
      SarkofagSplineTrajectoryActionMotor:
        joint_names: [joint1, joint2]
      SarkofagLimitDetector:
        console_notification_active: true
        pos_inc_limit: [0.0, 0.0]
        pos_limit_active: [false, false]
        pos_inc_limit_active: [false, false]
      SarkofagRegulator1:
        debug: false
        eint_dif: 0.01
        A: 0.412429378531
        BB0: 1.5569592
        BB1: 1.5028614
        current_reg_kp: 0.2
        output_multiplicator: 13.3
        max_output_current: 20
        max_desired_increment: 2000.0
        enc_res: 20000.0
      SarkofagRegulator2:
        debug: false
        eint_dif: 0.01
        A: 0.412429378531
        BB0: 1.5569592
        BB1: 1.5028614
        current_reg_kp: 0.2
        output_multiplicator: 13.3
        max_output_current: 20
        max_desired_increment: 2000.0
        enc_res: 20000.0
      SarkofagPortDoubleSplit_hw_to_reg:
        number_of_ports: 2
      SarkofagPortDoubleSplit_hw_cp:
        number_of_ports: 2
      SarkofagPortDoubleAggregate_hw_p:
        number_of_ports: 2
      SarkofagPortDoubleSum_1:
        number_of_ports: 1
      SarkofagPortDoubleSum_2:
        number_of_ports: 1
      SarkofagJntPub:
        joint_names: [SarkofagJoint1, SarkofagJoint2]
      SarkofagPortDoubleAggregate_im2rm:
        number_of_ports: 2
    </rosparam>
  </group>

</launch>
