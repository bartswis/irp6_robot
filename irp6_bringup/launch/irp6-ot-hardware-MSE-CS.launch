<launch>

  <include file="$(find rtt_ros)/launch/deployer.launch">
    <arg name="NAME" value="IRP6_OT_OROCOS_ROBOT"/>
    <arg name="DEPLOYER_ARGS" value="-s $(find irp6_bringup)/config/irp6-ot-MSE-CS.ops"/>
    <arg name="LOG_LEVEL" value="Warning"/>
    <arg name="DEBUG" value="false"/>
  </include>


  <include file="$(find irp6_bringup)/launch/irp6-ot.launch">
  </include>

  <group ns="IRP6_OT_OROCOS_ROBOT">
    <rosparam>
      Irp6scheme:
        scheme_name: irp6ot_manager
      Irp6HardwareInterface:
        test_mode: false
        timeouts_to_print: 3
      ATI3084:
        test_mode: false
      MotorStateExport_0:
        reg_number: 0
        pre_syn_export_: false
        debug: false
        filename: irp6-ot_0
      MotorStateExport_1:
        reg_number: 1
        pre_syn_export_: false
        debug: false
        filename: irp6-ot_1
      MotorStateExport_2:
        reg_number: 2
        pre_syn_export_: false
        debug: false
        filename: irp6-ot_2
      MotorStateExport_3:
        reg_number: 3
        pre_syn_export_: false
        debug: false
        filename: irp6-ot_3
      MotorStateExport_4:
        reg_number: 4
        pre_syn_export_: false
        debug: false
        filename: irp6-ot_4
      MotorStateExport_5:
        reg_number: 5
        pre_syn_export_: false
        debug: false
        filename: irp6-ot_5
      MotorStateExport_6:
        reg_number: 6
        pre_syn_export_: false
        debug: false
        filename: irp6-ot_6
      MotorStateExport_7:
        reg_number: 7
        pre_syn_export_: false
        debug: false
        filename: irp6-ot_7
    </rosparam>
  </group>

</launch>
